{
  "name": "enduro-stats",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "articles:add": "node ./scripts/articles.mjs --add",
    "precommit": "npm run test",
    "watch:server": "npm-watch server",
    "watch:client": "npm-watch client",
    "build-watch": "npm-watch build",
    "clean": "rimraf bundlemap-js.json server/dist/js server/dist/css",
    "client": "npm run clean && npm run build:css && npm run build:js && npm start",
    "server": "npm start",
    "start": "node index.js",
    "test": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=test tap --disable-coverage --allow-empty-coverage test/**",
    "test:integration": "NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=test tap --disable-coverage --allow-empty-coverage integration-test/*_test.js",
    "coverage": "NODE_ENV=test nyc npm test",
    "autotest": "NODE_ENV=test ./node_modules/tap/bin/run.js -w --no-coverage-report --no-check-coverage 'test/**'",
    "build:js": "NODE_OPTIONS=--openssl-legacy-provider webpack && cp src/js/sortable.min.js src/js/highcharts.js server/dist/js/",
    "build:css": "postcss src/css/main.css -o server/dist/css/bundle.css && postcss server/dist/css/bundle.css -o server/dist/css/bundle.css && cp src/css/bootstrap* server/dist/css/ && cp src/css/sort-table.css server/dist/css/ && postcss server/dist/css/sort-table.css -o server/dist/css/sort-table.css",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "lint": "standard --fix",
    "dev": "dvlp --port 8080 index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knuthaug/enduro-stats.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/knuthaug/enduro-stats/issues"
  },
  "homepage": "https://github.com/knuthaug/enduro-stats#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "7.24.0",
    "ansi-regex": ">=5.0.1",
    "assets-webpack-plugin": "3.9.7",
    "babel": "6.23.0",
    "babel-loader": "8.0.4",
    "bootstrap": "4.5.3",
    "command-line-args": "5.2.1",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "5.2.4",
    "css-what": ">=5.0.1",
    "cssnano": "5.0.5",
    "db-migrate": "0.11.12",
    "db-migrate-pg": "1.2.2",
    "deep-freeze": "0.0.1",
    "dvlp": "16.4.3",
    "feather-icons": "4.9.0",
    "glob-parent": ">=6.0.1",
    "got": ">=12.5.3",
    "gray-matter": "4.0.3",
    "i": ">=0.3.7",
    "jquery": "3.5.0",
    "jsdom": "21.1.1",
    "jsdom-global": "3.0.2",
    "just-extend": ">=4.0.0",
    "leaflet": "1.5.1",
    "leaflet-gpx": "1.4.0",
    "leaflet-plugins": "3.3.0",
    "marked": "4.1.0",
    "md5": "2.2.1",
    "nock": "11.3.4",
    "nodemon": "3.1.0",
    "npm-watch": "0.7.0",
    "nth-check": ">=2.0.1",
    "popper.js": "1.16.1",
    "postcss": "8.4.31",
    "postcss-cli": "8.3.1",
    "postcss-hash": "3.0.0",
    "postcss-import": "14.0.2",
    "rewire": "6.0.0",
    "rimraf": "2.6.2",
    "set-value": ">=4.0.1",
    "sinon": "7.4.2",
    "ssh2": "1.5.0",
    "standard": "16.0.4",
    "style-loader": "0.23.1",
    "supertest": "3.3.0",
    "supervisor": "latest",
    "tap": "18.7.0",
    "webpack": "5.94.0",
    "webpack-cli": "4.9.1",
    "detect-browser": "5.3.0",
    "web-vitals": "4.2.4"
  },
  "dependencies": {
    "convict": "6.2.4",
    "fastify-compress": "3.6.1",
    "fastify-formbody": "5.1.0",
    "fastify-static": "4.4.2",
    "neat-csv": "7.0.0",
    "date-fns": "4.1.0",
    "fastify": "5.2.0",
    "@fastify/compress": "8.0.1",
    "@fastify/formbody": "8.0.1",
    "@fastify/static": "8.0.3",
    "@fastify/view": "10.0.1",
    "handlebars": "4.7.8",
    "inquirer": "7.3.3",
    "morgan": "1.10.0",
    "needle": "3.2.0",
    "pg": "8.13.1",
    "pino": "9.6.0"
  },
  "watch": {
    "build": {
      "patterns": []
    },
    "client": {
      "patterns": [
        "server/*.js",
        "src/js",
        "src/css",
        "config",
        "views/",
        "views/partials",
        "views/layouts"
      ],
      "extensions": "js,json,css,handlebars",
      "ignore": [
        "node_modules",
        "server/dist/js",
        "server/dist/css"
      ]
    },
    "server": {
      "patterns": [
        "server/*.js",
        "views/",
        "views/partials",
        "views/layouts"
      ],
      "extensions": "js,json,css,handlebars",
      "ignore": [
        "node_modules",
        "server/dist/js",
        "server/dist/css"
      ]
    }
  },
  "standard": {
    "ignore": [
      "migrations/",
      "server/src/js"
    ]
  }
}
