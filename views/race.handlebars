<div class="row">
  <div class="col-2">
  </div>
  <div class="col-8">
    <h2>{{race.name}}</h2>
    <p>Dato: {{race.date}}</p>
  </div>
</div>
<div class="row">
  <div class="col-12">
    {{#each results}}
    <h3>{{@key}}</h3>

    <table class="table table-dark table-striped table-sm">
      <thead>
        <tr>
          <th scope="col">Plass</th>
          <th scope="col">Navn</th>
          {{#each ../stages}}
          <th scope="col">Etappe {{this}}</th>
          {{/each}}
          <th scope="col">Totaltid</th>
          <th scope="col">Etter vinner</th>
        </tr>
      </thead>
      <tbody>
        {{#each this}}
        <tr>
          <th scope="row">{{rank}}</th>
          <td>{{name}}</td>
          {{#each ../../stages}}
          <td>{{lookup ../this (propFor this 'time')}} <span class="stagerank"> ({{lookup ../this (propFor this 'rank')}})</span></td>
          {{/each}}
          <td>{{acc_time}}</td>
          <td>{{acc_time_behind}}</td>
        </tr>
        {{/each}}
        <tr>
      </tbody>
    </table>
    {{/each}}
  </div>
</div>



