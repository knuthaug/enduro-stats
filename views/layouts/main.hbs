<!doctype html>
  <html class="no-js" lang="no">

    <head>
      <meta charset="utf-8">
      <meta http-equiv="x-ua-compatible" content="ie=edge">
      <title>Norsk enduro</title>
      <meta name="description" content="">
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

      <link rel="manifest" href="site.webmanifest">
      <link rel="apple-touch-icon" href="icon.png">
      <link rel="stylesheet" href="/assets/css/bundle.css">
    </head>

    <body>

      <header>
        <nav class="navbar navbar-expand-md navbar-dark navbar-bg">
          <a class="navbar-brand" href="/">Norsk enduro</a>
          <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Vis meny">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarCollapse">
            <ul class="navbar-nav mr-auto">
              <li class="nav-item">
                <a class="nav-link {{#compare active '===' 'ritt'}}active{{/compare}}" href="/ritt">Ritt</a>
              </li>
              <li class="nav-item">
                <a class="nav-link {{#compare active '===' 'ryttere'}}active{{/compare}}" href="/ryttere">Ryttere</a>
              </li>
            </ul>
            <form class="form-inline mt-2 mt-md-0">
              <input class="form-control mr-sm-2" type="text" placeholder="søk" aria-label="Search">
              <button class="btn my-2 my-sm-0" type="submit">søk</button>
            </form>
          </div>
        </nav>
      </header>

      <main role="main container">
        <div class="position-relative p-3 p-md-3 m-md-3">
          {{{ body }}}
        </div>
      </main>
      <footer class="footer">
        <div class="container">
          <span class="text-muted">Knut Haugen @ 2018.</span>
        </div>
      </footer>
    </body>
    <script src="/assets/js/{{hashedAssets 'bundle.js'}}" crossorigin="anonymous"></script>
  </html>
